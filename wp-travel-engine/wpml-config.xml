			<key name="extra_service_title"/>
			<key name="discount_guide_open_title"/>
			<key name="summary_label"/>
			<key name="company_summary_label"/>
			<key name="reviewed_tour_text"/>
			<key name="excellent_label"/>
			<key name="vgood_label"/>
			<key name="average_label"/>
			<key name="poor_label"/>
			<key name="privacy_policy_msg"/>
			<key name="footer_copyright"/>
			<key name="departure">
				<key name="section_title"/>
			</key>
			<key name="trip_dates">
				<key name="number"/>
			</key>
		</key>
		<key name="wptravelengine_pricing_type">
			<key name="*"/>
		</key>
	</admin-texts>
	<shortcodes>
		<shortcode>
			<tag>THANK_YOU</tag>
		</shortcode>
		<shortcode>
			<tag>PLACE_ORDER</tag>
		</shortcode>
		<shortcode>
			<tag>BOOK_CONFIRMATION</tag>
		</shortcode>
	</shortcodes>
</wpml-config>
